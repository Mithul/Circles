<div class="checklist nested-fields">
  <%= f.error_notification %>
  <%= f.input :title %>
  <%= f.input :checked %>
  <%= f.input :user, :collection => User.all,:label_method => :name,:value_method => :id,:label => "Assign to" ,:include_blank => false %>
  <%= link_to_remove_association "Remove item", f %>
</div>